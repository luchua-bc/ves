search summaryfieldcollision {

  document summaryfieldcollision {

    field title type string {
      indexing: summary | index
    }

    field description type string {
      indexing: summary | index
    }
  }

  document-summary sum1 {
    summary f type string {
      source: title
    }
  }

  document-summary sum2 {
    summary f type string {
      source: description
    }
  }
}
