<services>
    <admin version="2.0">
         <adminserver hostalias="node1" />
    </admin>

    <routing version="1.0">
        <routingtable protocol="document">
            <hop name="foo" selector="bar/baz" />
            <hop name="foo" selector="baz/cox" />
        </routingtable>
        <services protocol="document">
            <service name="bar/baz" />
            <service name="baz/cox" />
        </services>
    </routing>
</services>
